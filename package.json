{
  "name": "turborepo-basic-yarn",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel --global-deps='.env' --force",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install",
    "migrate": "typeorm migration:run"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "esbuild": "^0.14.10",
    "esbuild-jest": "^0.5.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "jest-watch-typeahead": "^1.0.0",
    "lint-staged": "^12.1.3",
    "msw": "^0.36.3",
    "prettier": "^2.5.1",
    "turbo": "*"
  },
  "lint-staged": {
    "apps/**/*.{js,ts,jsx,tsx}": [],
    "*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^2.2.1",
    "@emotion/react": "^11.9.3",
    "find-up": "^6.3.0",
    "framer-motion": "^6.3.16",
    "next": "^12.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
